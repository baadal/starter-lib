{
  "name": "starter-lib",
  "version": "0.1.0",
  "description": "Starter kit for package development",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./cjs": "./dist/cjs/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "author": "Abhishek Raj <abhi@raj.me>",
  "license": "MIT",
  "scripts": {
    "build": "bash tasks/build.sh",
    "clean": "shx rm -rf dist",
    "prepub": "run-s --silent clean build",
    "pub": "npm run prepub && npm publish",
    "pub:test": "npm run prepub && npm publish --dry-run",
    "blame": "git log -S",
    "hist": "git log --oneline | grep -i",
    "follow": "git log --follow --oneline",
    "ncu": "ncu",
    "ncuu": "ncu -u"
  },
  "devDependencies": {
    "@babel/core": "7.15.5",
    "@babel/preset-env": "7.15.6",
    "@babel/preset-typescript": "7.15.0",
    "babel-loader": "8.2.2",
    "npm-run-all": "4.1.5",
    "shx": "0.3.3",
    "webpack": "5.53.0",
    "webpack-cli": "4.8.0"
  },
  "dependencies": {
    "core-js": "3.18.0"
  },
  "browserslist": [
    "chrome >= 23"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baadal/starter-lib.git"
  },
  "homepage": "https://github.com/baadal/starter-lib#readme"
}
